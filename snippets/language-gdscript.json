{
  ".source.gdscript": {
    "clasex": {
      "prefix": "clex",
      "body": "class ${1:ClassName} extends ${2:OtherClass}:\n\t\t${3}"
    },
    "class": {
      "prefix": "class",
      "body": "class ${1:ClassName}:\n\t\t${2}"
    },
    "elif": {
      "prefix": "elif",
      "body": "elif ${1:compare}:\n\t${2}"
    },
    "else": {
      "prefix": "else",
      "body": "else:\n\t${1}"
    },
    "for": {
      "prefix": "for",
      "body": "for ${1:i} in ${2:range_or_array}:\n\t${3}"
    },
    "func": {
      "prefix": "func",
      "body": "func ${1:method}(${2:args}):\n\t${3}"
    },
    "if": {
      "prefix": "if",
      "body": "if ${1:compare}:\n\t${2}"
    },
    "if_in": {
      "prefix": "ifin",
      "body": "if ${1:a} in ${2:array}:\n\t${3}"
    },
    "while": {
      "prefix": "while",
      "body": "while(${1:expression}):\n\t${2}"
    }
  }
}