{
  "name": "lang-gdscript",
  "version": "6.1.0",
  "description": "GDScript (Godot Scripting Language) grammar and completions for atom.io.",
  "main": "./lib/main",
  "activationHooks": [
    "lang-gdscript:grammar-used"
  ],
  "keywords": [
    "tscn",
    "tres",
    "gdscript",
    "godot"
  ],
  "repository": "https://github.com/PrestonKnopp/language-gdscript.git",
  "license": "",
  "engines": {
    "atom": ">=1.36.0 <2.0.0"
  },
  "dependencies": {
    "atom-languageclient": "^1.0.7",
    "tree-sitter-gdscript": "^1.1.0",
    "tree-sitter-godot-resource": "^0.2.1",
    "ws": "^7.4.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    }
  },
  "consumedServices": {
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "consumeSignatureHelp"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    },
    "console": {
      "versions": {
        "0.1.0": "consumeConsole"
      }
    }
  }
}
